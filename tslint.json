{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {},
	"rules": {
		"max-classes-per-file": [
			false
		],
		"eofline": true,
		"semicolon": [
			"never"
		],
		"quotemark": [
			"single",
			"jsx-single"
		],
		"indent": [
			true,
			"tabs",
			4
		]
	},
	"rulesDirectory": []
}