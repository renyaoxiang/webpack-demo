{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {},
	"rules": {
		"object-literal-sort-keys": false,
		"trailing-comma": [
			false
		],
		"arrow-parens": false,
		"ordered-imports": [
			false
		],
		"no-console": [
			"log"
		],
		"max-classes-per-file": [
			false
		],
		"eofline": true,
		"semicolon": [
			"never"
		],
		"quotemark": [
			"single",
			"jsx-single"
		],
		"indent": [
			true,
			"tabs",
			4
		]
	},
	"rulesDirectory": []
}