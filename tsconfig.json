{
	"compilerOptions": {
		"experimentalDecorators": true,
		"module": "amd",
		"moduleResolution": "node",
		"target": "es6",
		"lib": [
			"dom",
			"es2017"
		],
		"allowUnreachableCode": true,
		"allowUnusedLabels": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"noUnusedLocals": true,
		"sourceMap": true,
		"jsx": "react"
	}
}